<div class="user-form">

    <div class="form__input">
        <label class="user-form-label" for="floatingInput">Email address</label>
        <input type="text" class="form-control" id="floatingInput" value="{{this.Email}}" placeholder="Email"
            name="email">
    </div>

    <div class="form__input">
         <label class="user-form-label" for="floatingInput">First name</label>
        <input type="text" class="form-control" id="floatingInput" value="{{this.FirstName}}" placeholder="First Name"
            name="first_name">
    </div>

    <div class="form__input">
        <label class="user-form-label" for="floatingInput">Last name</label>
        <input type="text" class="form-control" id="floatingInput" value="{{this.LastName}}" placeholder="Last Name"
            name="last_name"> 
    </div>

    <div class="form__input">
        <label class="user-form-label" for="floatingSelect">Office name</label>
        <select class="form-select" id="floatingSelect" aria-label="Floating label select example" name="office">
            <option value="1">Abu dhabi</option>
            <option value="3">Cairo</option>
            <option value="4">Bahrain</option>
            <option value="5">Doha</option>
            <option value="6">Riyadh</option>
        </select>
    </div>

    <div class="form__input">
        <label class="user-form-label" for="floatingInput">Birthdate (dd/mm/yy)</label>
        <input type="text" class="form-control" id="floatingInput" value="{{this.Birthdate}}" placeholder="Birthdate"
            name="birthdate">
    </div>


    <div class="form__input">
        <label class="user-form-label" for="floatingInput">Password</label>
        <input type="text" class="form-control" id="floatingInput" value="{{this.Password}}" placeholder="Password"
            name="password">
    </div>

</div>

<div class="col-12">
    <button class="btn btn-primary" type="submit">Submit</button>
</div>